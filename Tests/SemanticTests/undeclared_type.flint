// RUN: %flintc %s --verify

contract Test {
  var userType: UDT
  var owner: T // expected-error {{Use of undeclared type 'T'}}
}

Test :: (any) {
  public init() {
    self.owner = T() // expected-error {{Function 'T' is not in scope or cannot be called using the caller capability 'any'}}
    self.userType = UDT()
  }
}

struct UDT {}
