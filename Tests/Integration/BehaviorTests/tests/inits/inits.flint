contract Inits {
  var a: Int
  var b: Address
  var s: String
  var s1: String = "foo"
  var t: T = T(0, false)
}

Inits :: caller <- (any) {
  public init(a: Int, s: String) {
    self.a = a
    self.b = caller
    self.s = s
  }

  public func getA() -> Int {
    return a
  }

  public func getB() -> Address {
    return b
  }

  public func getS() -> String {
    return s
  }

  public func getS1() -> String {
    return s1
  }

  public func setT(x: Int, y: Bool) mutates(t) {
    t = T(x, y)
  }

  public func getTx() -> Int {
    return t.x
  }

  public func getTy() -> Bool {
    return t.y
  }

  public func getTs() -> String {
    return t.s
  }
}

struct T {
  var x: Int
  var y: Bool
  let s: String = "test"
  
  public init(x: Int, y: Bool) {
    self.x = x
    self.y = y
  }
}
