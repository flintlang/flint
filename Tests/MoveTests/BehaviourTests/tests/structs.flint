struct Maybe {
  var element: Int
  var size: Int

  public init() {
    element = 0
    size = 0
  }

  func add(value: Int) mutates (element, size) {
    element = value
    size = 1
  }

  func get(n: Int) -> Int {
    if (self.size != 0) {
      return self.element
