// RUN: %flintc %s --dump-ast | %FileCheck %s --prefix CHECK-AST

contract Branching {}

Branching :: (any) {
// CHECK-AST: SpecialDeclaration
// CHECK-AST:   public
  public init() {}
  
  public func foo(a: Int) -> Int {
    if a > 6 {
      return 0
    }
    
    if a > 5 {
      return 1
    }

// CHECK-AST: IfStatement
// CHECK-AST:  BinaryExpression
// CHECK-AST:    identifier "a"
// CHECK-AST:    >
// CHECK-AST:    literal 4
// CHECK-AST:  IfStatement
// CHECK-AST:    BinaryExpression
// CHECK-AST:      identifier "a"
// CHECK-AST:      >
// CHECK-AST:      literal 3
// CHECK-AST:    ReturnStatement
// CHECK-AST:      literal 2
    if a > 4 {
      if a > 3 {
        return 2
      }
    }

    return 3
  }
}
